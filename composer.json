{
    "name": "vegvisir/grown",
    "autoload": {
        "psr-4": {
            "Vegvisir\\Grown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vegvisir\\Grown\\Tests\\": "tests/"
        }
    },
    "description": "Laravel package for ownership/grabability of models",
    "keywords": [
        "laravel", "ownership", "grabability", "php"
    ],
    "type": "library",
    "require": {
        "laravel/framework": "^5.4",
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpcov": "^4.0|^5.0",
        "php-coveralls/php-coveralls": "^2.1",
        "jenssegers/mongodb": "~2.0|~3.0"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Vegvisir Sp. z o.o.",
            "email": "vegvisir.for.all@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "vegvisir/trustnosql": "Laravel role/permission package working with MongoDB database"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vegvisir\\Grown\\GrownServiceProvider"
            ],
            "aliases": {
                "Grown": "Vegvisir\\Grown\\GrownFacade"
            }
        }
    }
}
